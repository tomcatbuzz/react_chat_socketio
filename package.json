{
  "name": "react-chat-socketio",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "express": "^4.16.3",
    "lodash": "^4.17.5",
    "prop-types": "^15.6.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-icons": "^2.2.7",
    "react-scripts": "1.1.4",
    "socket.io": "^2.1.0",
    "uuid": "^3.2.1"
  },
  "scripts": {
    "start": "node src/server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run react\"",
    "react": "react-scripts start",
    "server": "nodemon src/server/index.js",
    "build": "react-scripts build",
    "styles": "node-sass --output-style compressed --include-path scss src/styles/index.scss src/index.css",
    "styles:w": "nodemon -e scss -x \"npm run styles\"",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "node-sass": "^4.8.3",
    "nodemon": "^1.17.3"
  }
}
